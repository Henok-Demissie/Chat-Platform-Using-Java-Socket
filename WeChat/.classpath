<?xml version="1.0" encoding="UTF-8"?>
<classpath>
    <!-- JRE System Library -->
    <classpathentry 
        kind="con" 
        path="org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-22">
        <attributes>
            <!-- Indicates this is a modular Java runtime -->
            <attribute name="module" value="true"/>
        </attributes>
    </classpathentry>
    
    <!-- Output folder for compiled classes -->
    <classpathentry kind="output" path="bin"/>
</classpath>
